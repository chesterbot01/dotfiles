[user]
	name = Hongming Wang
	email = hongming.wang@shopify.com
	username = chesterbot01
[alias]
	co = checkout
	ci = commit
	ca = commit --amend -C HEAD
	br = branch --sort=-committerdate
	df = diff
	dfs = diff --staged
	lg = log --oneline --decorate --graph --abbrev-commit
	st = status -s
[push]
	default = simple
[pull]
	rebase = true
[color]
	ui = true
[sequence]
	editor = rebase-editor
[fetch]
	prune = true
